{
	"name": "bot",
	"module": "src/index.ts",
	"type": "module",
	"private": true,
	"scripts": {
		"start": "bun sapphire gl >/dev/null 2>&1 && bun run src/index.ts",
		"compile": "bun build --compile --outfile bot src/index.ts",
		"dev": "chokidar 'src/**/*.ts' 'shared/**/*.ts' -i 'src/**/_load.ts' --silent --initial --kill -c 'bun run start'",
		"lint": "xo --fix",
		"fmt": "prettier --write .",
		"gen": "sapphire g",
		"prepare": "husky",
		"prisma": "prisma",
		"sapphire": "sapphire"
	},
	"dependencies": {
		"@catppuccin/palette": "^1.7.1",
		"@fontsource-variable/nunito": "^5.2.6",
		"@logtape/file": "^1.0.4",
		"@logtape/logtape": "^1.0.4",
		"@logtape/pretty": "^1.0.4",
		"@multiavatar/multiavatar": "^1.0.7",
		"@prisma/client": "^6.11.1",
		"@sapphire/decorators": "^6.2.0",
		"@sapphire/discord.js-utilities": "^7.3.3",
		"@sapphire/framework": "^5.3.6",
		"@sapphire/utilities": "^3.18.2",
		"discord.js": "^14.21.0",
		"node_modules-path": "^2.1.0",
		"skia-canvas": "^2.0.2",
		"unique-names-generator": "^4.7.1"
	},
	"devDependencies": {
		"@sapphire/cli": "^1.9.3",
		"@types/bun": "^1.2.14",
		"@types/yargs": "^17.0.33",
		"chokidar-cli": "^3.0.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-sort-keys": "^2.3.5",
		"eslint-plugin-unused-imports": "^4.1.4",
		"husky": "^9.1.7",
		"prettier": "^3.6.2",
		"prettier-plugin-prisma": "^5.0.0",
		"prisma": "^6.11.1",
		"xo": "^1.2.1",
		"yargs": "^18.0.0"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"trustedDependencies": [
		"@prisma/client",
		"@prisma/engines",
		"prisma",
		"skia-canvas"
	]
}
